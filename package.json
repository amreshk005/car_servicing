{
  "name": "go_mechanic",
  "version": "1.0.0",
  "description": "this is the api version of go mechanic",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon server.js -w",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "watch:js": "parcel watch ./static/js/index.js --out-dir ./static/js/ --out-file bundle.js",
    "build:js": "parcel watch ./static/js/index.js --out-dir ./static/js/ --out-file bundle.js",
    "devserver": "live-server --entry-file=pricing.html",
    "watch:sass": "node-sass static/sass/main.scss static/css/index.css -w",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass static/sass/main.scss static/css/index.comp.css",
    "concat:css": "concat -o static/css/index.concat.css static/css/index.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' static/css/index.concat.css -o static/css/index.prefix.css",
    "compress:css": "node-sass static/css/index.prefix.css static/css/index.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "watchs:sass": "node-sass static/sass/main_pricing.scss static/css/pricing.css -w",
    "starts": "npm-run-all --parallel devserver watch:sass ",
    "compiles:sass": "node-sass static/sass/main_pricing.scss static/css/pricing.comp.css",
    "concats:css": "concat -o static/css/pricing.concat.css static/css/pricing.comp.css",
    "prefixs:css": "postcss --use autoprefixer -b 'last 10 versions' static/css/pricing.concat.css -o static/css/pricing.prefix.css",
    "compressPricing:css": "node-sass static/css/pricing.prefix.css static/css/pricing.css --output-style compressed",
    "builds:css": "npm-run-all compiles:sass concats:css prefixs:css compressPricing:css"
  },
  "author": "amresh kumar",
  "license": "ISC",
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concat": "^1.0.3",
    "consolidate": "^0.15.1",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ejs-extend": "0.0.1",
    "fast-csv": "^4.0.2",
    "jade": "^1.11.0",
    "live-server": "^1.2.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.4.2",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "pug": "^2.0.4",
    "slugify": "^1.3.6",
    "swig": "^1.4.2",
    "validator": "^12.1.0"
  },
  "engines": {
    "node": "^13.6.0"
  }
}
